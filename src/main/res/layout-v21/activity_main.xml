<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/KhongmainLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2a2727"
    tools:context=".MainActivity">


    <include layout="@layout/content_main" />


    <Button

        android:id="@+id/OrbitButtonBG"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/roundbutton"
        android:backgroundTint="#311a00"
        android:textColor="#b87333" />



    <Button
        android:id="@+id/PlanetOne"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:layout_above="@+id/OrbitButtonBG"

        android:layout_centerInParent="true"
        android:layout_marginBottom="11dp"
        android:background="@drawable/roundbutton"
        android:backgroundTint="@android:color/holo_blue_dark" />

    <Button
        android:id="@+id/PlanetTwo"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:layout_above="@+id/OrbitButtonBG"

        android:layout_centerInParent="true"
        android:layout_marginBottom="29dp"
        android:background="@drawable/roundbutton"
        android:backgroundTint="@android:color/holo_blue_dark" />

    <Button
        android:id="@+id/planetThree"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:layout_above="@+id/OrbitButtonBG"

        android:layout_centerInParent="true"
        android:layout_marginBottom="47dp"
        android:background="@drawable/roundbutton"
        android:backgroundTint="@android:color/holo_blue_dark" />

    <Button
        android:id="@+id/planetFour"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:layout_above="@+id/OrbitButtonBG"

        android:layout_centerInParent="true"
        android:layout_marginBottom="65dp"
        android:background="@drawable/roundbutton"
        android:backgroundTint="@android:color/holo_blue_dark" />

    <Button
        android:id="@+id/planetFive"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:layout_above="@+id/OrbitButtonBG"

        android:layout_centerInParent="true"
        android:layout_marginBottom="83dp"
        android:background="@drawable/roundbutton"
        android:backgroundTint="@android:color/holo_blue_dark" />

    <Button
        android:id="@+id/planetSix"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:layout_above="@+id/OrbitButtonBG"

        android:layout_centerInParent="true"
        android:layout_marginBottom="101dp"
        android:background="@drawable/roundbutton"
        android:backgroundTint="@android:color/holo_blue_dark" />

    <Button
        android:id="@+id/planetSeven"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:layout_above="@+id/OrbitButtonBG"

        android:layout_centerInParent="true"
        android:layout_marginBottom="118dp"
        android:background="@drawable/roundbutton"
        android:backgroundTint="@android:color/holo_blue_dark" />

    <Button
        android:id="@+id/planetEight"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:layout_above="@+id/OrbitButtonBG"

        android:layout_centerInParent="true"
        android:layout_marginBottom="136dp"
        android:background="@drawable/roundbutton"
        android:backgroundTint="@android:color/holo_blue_dark" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="42dp"
        android:layout_height="35dp"
        android:background="@android:color/transparent"
        android:contentDescription="@string/SettingsButton"
        android:tint="@color/copper"
        app:srcCompat="@drawable/settingss" />


    <Button
        android:id="@+id/OrbitButton"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/roundbutton"
        android:backgroundTint="#00000000"
        android:text="@string/orbit"
        android:textColor="#b87333" />

    <SeekBar
        android:id="@+id/seekBar1"
        android:layout_width="2dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="313dp"
        android:layout_marginEnd="-66dp"
        android:layout_marginBottom="372dp"
        android:max="100"
        android:progress="0"
        android:progressTint="@color/copper"
        android:rotation="270"
        android:thumb="@drawable/thumb" />


</RelativeLayout>